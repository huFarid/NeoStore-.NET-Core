
@{
    ViewData["Title"] = "ShowGroupProducts";
}

<h4>Group Products</h4>

<div class=" container">

    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4 col-lg-3 mb-4">
                <partial name="_ProductBox" model="item"></partial>
            </div>
        }
    </div>
   
</div>

